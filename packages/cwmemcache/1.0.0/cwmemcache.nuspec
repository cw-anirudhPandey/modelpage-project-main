<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>CWMemcache</id>
    <version>1.0.0</version>
    <authors>CWMemcache</authors>
    <owners>CWMemcache</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>CWMemcache is a Memcached client library for .NET Core. Usage: Add services.AddEnyimMemcached(...) and app.UseEnyimMemcached() in Startup. Add IMemcachedClient into constructor.</description>
    <tags>memcached cache</tags>
    <dependencies>
      <group targetFramework=".NETStandard2.0">
        <dependency id="Microsoft.AspNetCore.Http.Abstractions" version="2.0.1" exclude="Build,Analyzers" />
        <dependency id="Microsoft.Extensions.Caching.Abstractions" version="2.0.0" exclude="Build,Analyzers" />
        <dependency id="Microsoft.Extensions.Configuration.Abstractions" version="2.0.0" exclude="Build,Analyzers" />
        <dependency id="Microsoft.Extensions.Logging.Abstractions" version="2.0.0" exclude="Build,Analyzers" />
        <dependency id="Microsoft.Extensions.Options" version="2.0.0" exclude="Build,Analyzers" />
        <dependency id="Microsoft.Extensions.Options.ConfigurationExtensions" version="2.0.0" exclude="Build,Analyzers" />
        <dependency id="Newtonsoft.Json" version="10.0.3" exclude="Build,Analyzers" />
        <dependency id="Newtonsoft.Json.Bson" version="1.0.1" exclude="Build,Analyzers" />
      </group>
    </dependencies>
  </metadata>
</package>