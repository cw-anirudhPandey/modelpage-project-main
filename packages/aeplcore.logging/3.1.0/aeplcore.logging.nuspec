<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>AEPLCore.Logging</id>
    <version>3.1.0</version>
    <authors>AEPLCore.Logging</authors>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Fixed bug where logging was failing in full dot net framework</description>
    <releaseNotes>v2.0.0
    -------------
    Added log level updation

    v2.0.1,v1.0.4
    -------------
    LogError to use LogException when data is of type Exception (to fix memory leak)

    v2.0.1
    -------------
    Add moduleName to log4net global logging context

    v2.0.2
    -------------
    Fixed bug where logging fails when called from Application_start method in Global.aspx.cs

    v3.0.0
    -------------
    Update .net core to 3.1
    Remove support for .net core 2.1/2.0

    v3.1.0
    -------------
    Add support httpcontext logging for net core</releaseNotes>
    <dependencies>
      <group targetFramework=".NETFramework4.5.1">
        <dependency id="AEPLCore.Utils" version="3.2.0" exclude="Build,Analyzers" />
        <dependency id="Consul" version="0.7.2.6" exclude="Build,Analyzers" />
        <dependency id="Gelf4Net" version="3.0.0.14" exclude="Build,Analyzers" />
        <dependency id="Microsoft.AspNet.WebApi.Core" version="5.2.3" exclude="Build,Analyzers" />
        <dependency id="log4net" version="2.0.8" exclude="Build,Analyzers" />
      </group>
      <group targetFramework=".NETFramework4.5.2">
        <dependency id="AEPLCore.Utils" version="3.2.0" exclude="Build,Analyzers" />
        <dependency id="Consul" version="0.7.2.6" exclude="Build,Analyzers" />
        <dependency id="Gelf4Net" version="3.0.0.14" exclude="Build,Analyzers" />
        <dependency id="Microsoft.AspNet.WebApi.Core" version="5.2.3" exclude="Build,Analyzers" />
        <dependency id="log4net" version="2.0.8" exclude="Build,Analyzers" />
      </group>
      <group targetFramework=".NETCoreApp3.1">
        <dependency id="AEPLCore.Utils" version="3.2.0" exclude="Build,Analyzers" />
        <dependency id="Consul" version="0.7.2.6" exclude="Build,Analyzers" />
        <dependency id="Gelf4Net" version="3.0.0.14" exclude="Build,Analyzers" />
        <dependency id="log4net" version="2.0.8" exclude="Build,Analyzers" />
      </group>
    </dependencies>
    <frameworkAssemblies>
      <frameworkAssembly assemblyName="System.Web" targetFramework=".NETFramework4.5.1, .NETFramework4.5.2" />
    </frameworkAssemblies>
  </metadata>
</package>